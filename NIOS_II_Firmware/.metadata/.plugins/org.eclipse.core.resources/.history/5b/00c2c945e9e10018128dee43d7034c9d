#include "alt_types.h"

#include "system.h"
#include "altera_avalon_pio_regs.h"

#define DELAY                32000

int main (void)
{
	alt_32 port_data = 0;
	alt_32 i = 0;

	for (;;)
	{
		IOWR_ALTERA_AVALON_PIO_DATA(PIO_0_BASE, port_data);
		for (i = 0; i < DELAY; i++);
		port_data ^= 0x3;
	}
}
